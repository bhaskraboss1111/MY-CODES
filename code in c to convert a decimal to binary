#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int arr[100];
   
    int i=0;
    while(n>0)
    {
        if(n%2==0)
        {
            arr[i]=0;
            i=i+1;
        }
        else
        {
            arr[i]=1;
            i=i+1;
        }
        n=n/2;
    }
    for(i=i-1;i>-1;i--)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}
